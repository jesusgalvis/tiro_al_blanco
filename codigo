
#include<stdio.h>
#include<stdlib.h>
#define a 11


void llenar1(int m1[][a]);
void imprimir1(int m1[][a]);
void llenar2(int m1[][a]);
void imprimir2(int m1[][a]);
void disparo(int v[a][a],int x,int y);


int main(){
	
	
	
	int m[a][a];
	
	llenar1(m);
	imprimir1(m);
	

	
	


	return 0;
}

void llenar1(int m1[][a]){
	
	int i,j,x,y,aux=0,z;
	
	for (i=0;i<a;i++){
		for (j=0;j<a;j++){
			m1[i][j]=220;
		}
	}
	for (i=1;i<a-1;i++){
		for (j=1;j<a-1;j++){
			m1[i][j]=0;
		}
	}
	for (i=2;i<a-2;i++){
		for (j=2;j<a-2;j++){
			m1[i][j]=61;
		}
	}
		for (i=3;i<a-3;i++){
		for (j=3;j<a-3;j++){
			m1[i][j]=0;
		}
	}
	
		for (i=4;i<a-4;i++){
		for (j=4;j<a-4;j++){
			m1[i][j]=61;
		}
	}
	
	m1[5][5]=0;
	
	srand(time(NULL));
		z=1+rand()%100;
		
		if(z%2==0){
			do{
					aux=0;
						srand(time(NULL));
						x=1+rand()%10;
				
						srand(time(NULL));
						y=1+rand()%10;
						
					
					if(m1[x][y]==220){
						aux=1;
					}else{
					m1[x][y]=42;
					}
					printf("\r[%d %d]",x,y);

	
			}while(aux==1);
			
		}else{
			
				do{
					aux=0;
						srand(time(NULL));
						x=1+rand()%10;
				
					do{
						srand(time(NULL));
						y=1+rand()%10;
					}while(x==y);	
					
					if(m1[x][y]==220){
						aux=1;
					}else{
					m1[x][y]=42;
					}
					printf("\r[%d %d]",x,y);

	
			}while(aux==1);
			
		}


	
}

void imprimir1(int m1[][a]){
	
	int i,j;
	printf("\n");
	for(i=0;i<a;i++){
		
		for(j=0;j<a;j++){
			printf("%c ",m1[i][j]);
		}
		printf("\n");
	}

	
}


void disparo(int v[a][a],int x,int y){
	
	int i,j;
	
	for(i=0;i<a;i++){
		
		for(j=0;j<a;j++){
			printf("%c ",v[x][y]);
		}
		printf("\n");
	}
	
}


